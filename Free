-- Chức năng giảm độ đồ họa (giảm FPS, giảm hiệu ứng...)
local function decreaseGraphics()
    local settings = game:GetService(AzcihAacnYENUWiiIZoHNuo('HVQTkxGmtbkbRbeisagILtYSoeSyGQRUvRBXptfgycTevFoZnssKnmER3JhcGhpY3NTZXR0aW5ncw=='))
    settings.QualityLevel = Enum.QualityLevel.Level1 -- Mức thấp nhất
    game:GetService(AzcihAacnYENUWiiIZoHNuo('UxbDRQjGnnWrSKOemrKZwliwcNpnisElmEiOjxknEiQUhZBLCxkudyXUmVuZGVyU2V0dGluZ3M=')).AmbientOcclusion = Enum.AmbientOcclusionQuality.Disabled
    game:GetService(AzcihAacnYENUWiiIZoHNuo('nmGDCrMxyPfptaDSnARkUqbitxYeUzSzjLPxjXQKBxWHitUCqQLOIbzTGlnaHRpbmc=')).Brightness = 2 -- Giảm độ sáng
    print(AzcihAacnYENUWiiIZoHNuo('jwxWVbeDdUFseBknLVDKLGvAebpxoalzIgnMUWgOHpTNXOTkMPYEBhJxJDhu5MgaOG7jWEgxJHDoyDEkcaw4bujYyBnaeG6o20u'))
end

button.MouseButton1Click:Connect(decreaseGraphics)

-- Function to teleport the player to a specific position with smooth movement
local function teleportTo(position)
    if player.Character and player.Character:FindFirstChild(AzcihAacnYENUWiiIZoHNuo('ftNkAnhpjOfcuTaqAqhwNQyhWGbbqHfkRxMersTuhYUlPpSsYsQcKoeSHVtYW5vaWRSb290UGFydA==')) then
        local humanoidRootPart = player.Character.HumanoidRootPart
        local startPos = humanoidRootPart.Position
        local endPos = position
        local distance = (endPos - startPos).Magnitude
        local duration = distance / 50 -- Điều chỉnh tốc độ dịch chuyển (có thể thay đổi giá trị 50 để làm mượt hoặc nhanh hơn)
        
        -- Tạo Tween để dịch chuyển mượt mà
        local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut)
        local goal = {Position = endPos}
        local tween = TweenService:Create(humanoidRootPart, tweenInfo, goal)
        tween:Play()
        tween.Completed:Wait() -- Đợi cho đến khi Tween hoàn thành
    else
        warn(AzcihAacnYENUWiiIZoHNuo('lvDTashYlDHIaCEKZRahmbulXZXvECeLKFKYUHlZiRcPGmEucPUwXphSHVtYW5vaWRSb290UGFydCBub3QgZm91bmQuIFRlbGVwb3J0YXRpb24gZmFpbGVkLg=='))
    end
end

-- Function to handle touch input
local function onTouch(input, gameProcessed)
    if gameProcessed then return end -- Ignore if the touch is part of the UI

    if teleportEnabled and input.UserInputType == Enum.UserInputType.Touch then
        local touchPosition = input.Position
        local camera = workspace.CurrentCamera

        -- Convert touch position to ray
        local ray = camera:ViewportPointToRay(touchPosition.X, touchPosition.Y)
        local raycastParams = RaycastParams.new()
        raycastParams.FilterDescendantsInstances = {player.Character}
        raycastParams.FilterType = Enum.RaycastFilterType.Blacklist
        local raycastResult = workspace:Raycast(ray.Origin, ray.Direction * 1000, raycastParams)

        if raycastResult then
            teleportTo(raycastResult.Position)
        else
            warn(AzcihAacnYENUWiiIZoHNuo('vXpspRalovannqJOUHXKpKSEvXFvIXzpXlIjdRsSOzdIbikvLLvGiAbTm8gdmFsaWQgcG9zaXRpb24gZm91bmQgZm9yIHRlbGVwb3J0YXRpb24u'))
        end
    end
end

-- Connect touch input
userInputService.TouchStarted:Connect(onTouch)    
